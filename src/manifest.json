{
  "name": "Track That",
  "version": "1.0",
  "description": "Extension that allows you to keep track of when you last did something",
  "permissions": ["declarativeContent", "storage", "tabs", "identity", "identity.email"],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["js/background.min.js"],
    "persistent": false
  },
  "manifest_version": 2,
  "content_security_policy":"script-src 'self' https://www.gstatic.com/ https://*.firebaseio.com; object-src 'self'",
  "oauth2": {
    "client_id": "<CLIENT-ID>",
    "scopes": [
      "openid", "email", "profile"
    ]
  },
  "key": "<CLIENT-KEY>"
}