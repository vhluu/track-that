{
  "name": "Track That",
  "version": "1.0",
  "description": "Extension that allows you to keep track of when you last did something",
  "permissions": ["declarativeContent", "storage", "tabs"],
  "background": {
    "scripts": ["bundle.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://cdn.firebase.com https://*.firebaseio.com; object-src 'self'"
}
