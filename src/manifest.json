{
  "name": "Track That",
  "version": "1.0.1",
  "description": "Keep track of your habits or when you last did something with Track That",
  "permissions": ["declarativeContent", "storage", "identity", "identity.email"],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["js/background.min.js"],
    "persistent": false
  },
  "manifest_version": 2,
  "content_security_policy":"script-src 'self' https://www.gstatic.com/ https://*.firebaseio.com; object-src 'self'",
  "oauth2": {
    "client_id": "<CLIENT-ID>",
    "scopes": [
      "openid", "email", "profile"
    ]
  },
  "key": "<CLIENT-KEY>",
  "icons": {
    "128": "favicon-128.png"
  }
}