{
  "name": "Track That",
  "version": "1.0",
  "description": "Extension that allows you to keep track of when you last did something",
  "permissions": ["declarativeContent", "storage", "tabs"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
